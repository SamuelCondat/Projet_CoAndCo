<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.corp.tajine.la_cacalculatrice">
<supports-screens
    android:largeScreens="true"
    android:normalScreens="true"
    android:smallScreens="true"
    android:xlargeScreens="true"
    />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/caca"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewTrophyActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".ViewModeActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".ViewCalculActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Cacalculatrice"
            android:screenOrientation="portrait">
        </activity>
    </application>
</manifest>